{
  "name": "Star-Wars-Forum",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",  
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt": "^1.0.3",
    "connect-mongo": "^2.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "moment": "^2.19.2",
    "mongoose": "^4.13.5",
    "npm-run-all": "^4.1.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-twitter-widgets": "^1.7.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "node-sass": "^4.7.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "build-css": "node-sass src/assets/sass/style.scss -o src/assets/css/",
    "watch-css": "node-sass src/assets/sass/style.scss -o src/assets/css/ --watch --recursive",
    "start": "node server.js",    
    "start-js": "react-scripts start",
    "start-dev": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001"
}
